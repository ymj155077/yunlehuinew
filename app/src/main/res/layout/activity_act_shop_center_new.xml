<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Write"
    android:orientation="vertical"
    tools:context=".ui.act.ActComPur.ActShopCenterNew">

    <include layout="@layout/toolbar_push_off" />


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">


        <LinearLayout

            android:id="@+id/lin_new"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.example.YunleHui.view.ObservableScrollView
                    android:id="@+id/sv_contentView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/Write"
                    android:fillViewport="true"
                    android:scrollbars="none">

                    <LinearLayout
                        android:id="@+id/ll_contentView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/Write"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_140"
                            android:background="@color/all_background"
                            android:gravity="center"
                            android:orientation="horizontal">


                            <ImageView
                                android:id="@+id/shopImg"
                                android:layout_width="@dimen/dp_80"
                                android:layout_height="@dimen/dp_80"
                                android:layout_marginStart="@dimen/dp_12"
                                android:scaleType="fitXY"
                                android:src="@drawable/timg" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_80"
                                android:layout_marginStart="@dimen/dp_12"
                                android:orientation="vertical">


                                <TextView
                                    android:id="@+id/text_name"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="商家"
                                    android:textColor="@color/black33"
                                    android:textSize="@dimen/sp_17"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/text_tel"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_4"
                                    android:text="电话："
                                    android:textColor="@color/black33"
                                    android:textSize="@dimen/sp_14" />

                                <TextView
                                    android:id="@+id/text_address"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_marginTop="@dimen/dp_4"
                                    android:layout_weight="1"
                                    android:text="地址："
                                    android:textColor="@color/black33"
                                    android:textSize="@dimen/sp_14" />

                            </LinearLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_topView"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_47"
                            android:gravity="start"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/tab_radiogroup"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/dp_12"
                                android:background="@color/Write"
                                android:orientation="horizontal">
                                <!-- checked="true"是指默认被选中
                                android:button="@null" 取消RaidoButton原来的圆按钮样式
                                类似的还有Android:background="@null"：即控件自带的背景设为空 -->

                                <LinearLayout
                                    android:id="@+id/lin_commodity"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/radio_1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="商品"
                                        android:textColor="@color/black33"
                                        android:textSize="@dimen/sp_17" />

                                    <View
                                        android:id="@+id/view_commodity"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/dp_2"
                                        android:layout_marginTop="@dimen/dp_4"
                                        android:background="@color/login" />


                                </LinearLayout>

                                <View
                                    android:layout_width="@dimen/dp_20"
                                    android:layout_height="@dimen/dp_1" />


                                <LinearLayout
                                    android:id="@+id/lin_shop"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/radio_2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="商家"
                                        android:textColor="@color/black33"
                                        android:textSize="@dimen/sp_17" />


                                    <View
                                        android:id="@+id/view_shop"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/dp_2"
                                        android:layout_marginTop="@dimen/dp_4"
                                        android:background="@color/login"
                                        android:visibility="invisible" />


                                </LinearLayout>

                            </LinearLayout>


                        </LinearLayout>

                        <!--商品-->

                        <RelativeLayout

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:descendantFocusability="blocksDescendants">

                            <android.support.v7.widget.RecyclerView
                                android:id="@+id/xre_mall"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:focusable="false"
                                android:nestedScrollingEnabled="false" />


                        </RelativeLayout>


                        <!--商家-->


                        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                            android:id="@+id/lin_shops"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/Write"
                            android:orientation="vertical"
                            android:visibility="gone">


                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_1"
                                android:background="@color/all_background" />


                            <LinearLayout

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">


                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_12"
                                    android:layout_marginTop="@dimen/dp_22"
                                    android:layout_marginBottom="@dimen/dp_22"
                                    android:src="@drawable/dingwei" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_6"
                                    android:text="地址： "
                                    android:textColor="@color/black99"
                                    android:textSize="@dimen/sp_17" />


                                <TextView
                                    android:id="@+id/text_ShopAddress"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="江北区新主路"
                                    android:textColor="@color/black33"
                                    android:textSize="@dimen/sp_17" />

                                <LinearLayout
                                    android:id="@+id/lin_cell"
                                    android:layout_width="@dimen/dp_50"
                                    android:layout_height="match_parent"
                                    android:gravity="center">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="@dimen/dp_12"
                                        android:src="@drawable/dianhua_" />


                                </LinearLayout>


                            </LinearLayout>


                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_1"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                android:background="@color/all_background" />


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_12"
                                    android:layout_marginTop="@dimen/dp_22"
                                    android:layout_marginBottom="@dimen/dp_22"
                                    android:src="@drawable/zizhi" />


                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_6"
                                    android:text="商家经营资质"
                                    android:textColor="@color/black33"
                                    android:textSize="@dimen/sp_17" />


                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_1"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                android:background="@color/all_background" />


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_12"
                                    android:layout_marginTop="@dimen/dp_12"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/peisong" />


                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/dp_6"
                                        android:text="配送服务：自主选择配送服务"
                                        android:textColor="@color/black99"
                                        android:textSize="@dimen/sp_17" />


                                </LinearLayout>


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_36"
                                    android:layout_marginTop="@dimen/dp_6"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="①"
                                        android:textColor="#707070"
                                        android:textSize="@dimen/sp_17" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text=" 门店自提"
                                        android:textColor="@color/black_333333"
                                        android:textSize="@dimen/sp_17" />


                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/dp_22"
                                        android:text="②"
                                        android:textColor="#707070"
                                        android:textSize="@dimen/sp_17" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text=" 社区点自提"
                                        android:textColor="@color/black_333333"
                                        android:textSize="@dimen/sp_17" />


                                </LinearLayout>


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_36"
                                    android:layout_marginTop="@dimen/dp_4"
                                    android:layout_marginBottom="@dimen/dp_6"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="①"
                                        android:textColor="#707070"
                                        android:textSize="@dimen/sp_17" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text=" 商家配送"
                                        android:textColor="@color/black_333333"
                                        android:textSize="@dimen/sp_17" />


                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/dp_22"
                                        android:text="②"
                                        android:textColor="#707070"
                                        android:textSize="@dimen/sp_17" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text=" 社区配送"
                                        android:textColor="@color/black_333333"
                                        android:textSize="@dimen/sp_17" />


                                </LinearLayout>


                            </LinearLayout>


                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_1"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                android:background="@color/all_background" />


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_12"
                                    android:layout_marginTop="@dimen/dp_22"
                                    android:layout_marginBottom="@dimen/dp_22"
                                    android:src="@drawable/shijian" />


                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_6"
                                    android:text="营业时间"
                                    android:textColor="@color/black33"
                                    android:textSize="@dimen/sp_17" />


                                <TextView
                                    android:id="@+id/text_openTime"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_2"
                                    android:text=" 09：00-23：40 "
                                    android:textColor="@color/black99"
                                    android:textSize="@dimen/sp_17" />


                            </LinearLayout>


                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_1"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                android:background="@color/all_background" />
                        </LinearLayout>


                    </LinearLayout>
                </com.example.YunleHui.view.ObservableScrollView>


                <LinearLayout
                    android:id="@+id/ll_fixedView"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_47"
                    android:orientation="vertical" />


            </RelativeLayout>

        </LinearLayout>


        <ScrollView
            android:id="@+id/scr_mall"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/Write"
            android:scrollbars="none"
            android:visibility="gone">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/img_view"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_187"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_12"
                    android:scaleType="fitXY" />


                <TextView
                    android:id="@+id/text_shopname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_12"
                    android:text="万家水果店"
                    android:textColor="@color/black_333333"
                    android:textSize="@dimen/sp_17" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_7"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="￥"
                        android:textColor="@color/login"
                        android:textSize="@dimen/sp_17" />

                    <TextView
                        android:id="@+id/text_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0.00"
                        android:textColor="@color/login"
                        android:textSize="@dimen/sp_24"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/text_yuan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_4"
                        android:text="原价 0.00"
                        android:textColor="@color/black99"
                        android:textSize="@dimen/sp_14" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_36"
                        android:text="月销   "
                        android:textColor="@color/black99"
                        android:textSize="@dimen/sp_14" />

                    <TextView
                        android:id="@+id/text_xiao"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="@color/black99"
                        android:textSize="@dimen/sp_14" />


                </LinearLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/all_background" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="详细信息"
                    android:textColor="@color/black_333333"
                    android:textSize="@dimen/sp_17" />


                <TextView
                    android:id="@+id/text_context"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_7"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:lineSpacingExtra="@dimen/dp_4"
                    android:text="云南野生菌汇虾仁、干腰河肠、小菜汤、手工苞 谷粑包2-3人餐位费 套餐二：火焰鸡、来凤鱼 小菜汤、花甲、手工苞谷粑包3-5人"
                    android:textColor="@color/black99"
                    android:textSize="@dimen/sp_16" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_47"
                    android:layout_marginEnd="@dimen/dp_16"
                    android:layout_marginBottom="@dimen/dp_100"
                    android:gravity="end"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_jian_detail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/jianhaohui" />


                    <TextView
                        android:id="@+id/text_number_detail"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/dp_12"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:gravity="center_vertical"
                        android:text="0"
                        android:textColor="@color/zhuce"
                        android:textSize="@dimen/sp_24" />


                    <ImageView
                        android:id="@+id/img_add_detail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/jiahaoxuanzhong" />

                </LinearLayout>


                <!--加入购物车-->


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginTop="@dimen/dp_47"
                    android:layout_marginEnd="@dimen/dp_17"
                    android:background="@drawable/baou"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_22"
                        android:layout_marginTop="@dimen/dp_6"
                        android:layout_marginEnd="@dimen/dp_22"
                        android:layout_marginBottom="@dimen/dp_6"
                        android:text="加入购物车"
                        android:textColor="@color/Write"
                        android:textSize="@dimen/sp_17" />

                </LinearLayout>


            </LinearLayout>


        </ScrollView>


        <LinearLayout
            android:id="@+id/lin_mall"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/lin_pop_type"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:alpha="0.5"
                android:background="#000000"
                android:clickable="true"
                android:orientation="vertical">


            </LinearLayout>


            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/mg"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_13"
                    android:layout_marginTop="@dimen/dp_13"
                    android:layout_marginBottom="@dimen/dp_13"
                    android:text="购物车"
                    android:textColor="@color/black_333333"
                    android:textSize="@dimen/sp_17" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:background="@color/all_background" />


                <ListView
                    android:id="@+id/list_gou"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:divider="@null"
                    android:scrollbars="none" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_10"
                    android:background="@color/all_background" />

            </LinearLayout>


        </LinearLayout>


    </FrameLayout>


    <LinearLayout

        android:id="@+id/lin_boom_gou"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@color/Write"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_7"
            android:layout_marginBottom="@dimen/dp_9"
            android:layout_weight="1"
            android:background="@drawable/bamg"
            android:gravity="center_vertical">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="@dimen/dp_36"
                    android:layout_height="@dimen/dp_34"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/dp_26"
                    android:layout_marginEnd="@dimen/dp_6"
                    android:src="@drawable/he" />


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:background="@drawable/bayou"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/text_Num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_3"
                        android:layout_marginTop="@dimen/dp_2"
                        android:layout_marginEnd="@dimen/dp_3"
                        android:layout_marginBottom="@dimen/dp_2"
                        android:gravity="center"
                        android:minWidth="@dimen/dp_10"
                        android:text="0"
                        android:textColor="@color/Write"
                        android:textSize="@dimen/sp_11" />

                </LinearLayout>

            </FrameLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_2"
                android:layout_marginBottom="@dimen/dp_2"
                android:orientation="vertical">


                <LinearLayout

                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="￥"
                        android:textColor="@color/Write"
                        android:textSize="@dimen/sp_14" />

                    <TextView
                        android:id="@+id/text_sum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0.00"
                        android:textColor="@color/Write"
                        android:textSize="@dimen/sp_18"
                        android:textStyle="bold" />


                </LinearLayout>


