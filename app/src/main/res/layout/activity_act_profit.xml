<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:background="@color/all_background"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.YunleHui.ui.act.actme.ActProfit">


    <include layout="@layout/toolbar_push_off" />




<LinearLayout
    android:layout_marginTop="@dimen/dp_12"
    android:layout_marginEnd="@dimen/dp_12"
    android:layout_marginStart="@dimen/dp_12"
    android:background="@drawable/beijing"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_185"
    android:orientation="vertical">



    <LinearLayout

        android:gravity="bottom"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:layout_marginStart="@dimen/dp_16"
            android:textSize="@dimen/sp_24"
            android:textColor="@color/Write"
            android:text="￥ "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_16" />

        <TextView
            android:id="@+id/text_all"
            android:textStyle="bold"
            android:textSize="@dimen/sp_36"
            android:textColor="@color/Write"
            android:text="0.00"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <View
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="1dp"/>


        <TextView
            android:textStyle="bold"
            android:layout_marginEnd="@dimen/dp_15"
            android:textSize="@dimen/sp_24"
            android:textColor="@color/Write"
            android:text="皮皮虾"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_15" />



        <com.makeramen.roundedimageview.RoundedImageView
            android:layout_marginTop="@dimen/dp_24"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/img_head"
            android:layout_width="@dimen/dp_47"
            android:layout_height="@dimen/dp_47"
        android:layout_marginEnd="@dimen/dp_12"
            android:scaleType="fitXY"
            android:src="@drawable/timg"
            app:riv_corner_radius="@dimen/dp_12"
            app:riv_mutate_background="true"
            android:layout_marginRight="@dimen/dp_12" />



    </LinearLayout>

    <TextView
        android:layout_marginTop="@dimen/dp_2"
        android:layout_marginStart="@dimen/dp_16"
        android:textSize="@dimen/sp_13"
        android:textColor="@color/Write"
        android:text="佣金总收益（元）"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16" />


<View
    android:layout_weight="1"
    android:layout_width="1dp"
    android:layout_height="0dp"/>


    <LinearLayout
        android:layout_marginBottom="@dimen/dp_25"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/text_zuo"
                android:layout_marginTop="@dimen/dp_2"
                android:layout_marginStart="@dimen/dp_16"
                android:textSize="@dimen/sp_18"
                android:textColor="@color/Write"
                android:text="￥0.00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_16" />

            <TextView
                android:layout_marginTop="@dimen/dp_2"
                android:layout_marginStart="@dimen/dp_16"
                android:textSize="@dimen/sp_10"
                android:textColor="@color/Write"
                android:text="已结算（元）"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_16" />
        </LinearLayout>


        <View
            android:layout_marginEnd="@dimen/dp_20"
            android:layout_marginStart="@dimen/dp_20"
            android:background="@color/Write"
            android:layout_width="@dimen/dp_1"
            android:layout_height="match_parent"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/text_you"
                android:layout_marginTop="@dimen/dp_2"
                android:textSize="@dimen/sp_18"
                android:textColor="@color/Write"
                android:text="￥0.00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
          />

            <TextView
                android:layout_marginTop="@dimen/dp_2"
                android:textSize="@dimen/sp_10"
                android:textColor="@color/Write"
                android:text="未结算（元）"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
             />
        </LinearLayout>

    </LinearLayout>









</LinearLayout>










<LinearLayout
    android:layout_marginTop="@dimen/dp_12"
    android:layout_marginEnd="@dimen/dp_12"
android:layout_marginStart="@dimen/dp_12"
    android:background="@drawable/backimg"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:id="@+id/lin_Team"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_79">

            <ImageView
                android:src="@drawable/q"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView

                android:layout_marginStart="@dimen/dp_6"
                android:textSize="@dimen/sp_17"
                android:textColor="@color/black33"
                android:text="我的团队"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_6" />

        </LinearLayout>

        <View
            android:background="@color/all_background"
            android:layout_width="@dimen/dp_1"
            android:layout_height="match_parent"/>

        <LinearLayout
            android:id="@+id/lin_customer"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_79">

            <ImageView
                android:src="@drawable/zx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView

                android:layout_marginStart="@dimen/dp_6"
                android:textSize="@dimen/sp_17"
                android:textColor="@color/black33"
                android:text="我的顾客"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_6" />

        </LinearLayout>

    </LinearLayout>


    <View
        android:background="@color/all_background"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:id="@+id/lin_share"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_79">

            <ImageView
                android:src="@drawable/as"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView

                android:layout_marginStart="@dimen/dp_6"
                android:textSize="@dimen/sp_17"
                android:textColor="@color/black33"
                android:text="我的分享"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_6" />

        </LinearLayout>

        <View
            android:background="@color/all_background"
            android:layout_width="@dimen/dp_1"
            android:layout_height="match_parent"/>

        <LinearLayout
            android:id="@+id/lin_Commission"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_79">

            <ImageView
                android:src="@drawable/chonghi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView

                android:layout_marginStart="@dimen/dp_6"
                android:textSize="@dimen/sp_17"
                android:textColor="@color/black33"
                android:text="佣金明细"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_6" />

        </LinearLayout>

    </LinearLayout>





</LinearLayout>











</LinearLayout>
